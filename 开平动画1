<html lang="zh-CN">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>kkcode - AI Coding CLI</title>
<style>
  * { margin: 0; padding: 0; box-sizing: border-box; }
  body {
    width: 100vw; height: 100vh; overflow: hidden;
    background: #0a0a0f; font-family: 'Courier New', monospace;
    display: flex; justify-content: center; align-items: center;
  }
  canvas#main { position: absolute; top: 0; left: 0; z-index: 1; }
  canvas#rain { position: absolute; top: 0; left: 0; z-index: 0; }
  canvas#fx { position: absolute; top: 0; left: 0; z-index: 2; pointer-events: none; }

  .scanlines {
    position: fixed; top: 0; left: 0; width: 100%; height: 100%;
    background: repeating-linear-gradient(0deg, transparent, transparent 2px,
      rgba(0,255,200,0.02) 2px, rgba(0,255,200,0.02) 4px);
    pointer-events: none; z-index: 10;
  }
  .glitch-overlay {
    position: fixed; top: 0; left: 0; width: 100%; height: 100%;
    pointer-events: none; z-index: 9; opacity: 0;
  }
  .pixel-grid {
    position: fixed; top: 0; left: 0; width: 100%; height: 100%;
    pointer-events: none; z-index: 8; opacity: 0;
    background-image:
      linear-gradient(rgba(0,255,255,0.03) 1px, transparent 1px),
      linear-gradient(90deg, rgba(0,255,255,0.03) 1px, transparent 1px);
    background-size: 6px 6px;
  }
  .pixel-grid.show { animation: gridIn 2s ease-out forwards; }
  @keyframes gridIn { 0%{opacity:0} 30%{opacity:1} 100%{opacity:0.25} }

  .vignette {
    position: fixed; top: 0; left: 0; width: 100%; height: 100%;
    background: radial-gradient(ellipse at center, transparent 50%, rgba(0,0,0,0.6) 100%);
    pointer-events: none; z-index: 7;
  }

  .text-layer {
    position: fixed; bottom: 0; left: 0; width: 100%;
    display: flex; flex-direction: column; align-items: center;
    padding-bottom: 12vh; z-index: 5; pointer-events: none;
  }
  .brand {
    font-size: 52px; font-weight: bold; letter-spacing: 14px;
    color: #0ff; opacity: 0;
    text-shadow: 0 0 10px #0ff, 0 0 40px #0ff, 0 0 80px rgba(0,255,255,0.4);
  }
  .brand.show { animation: brandIn .8s cubic-bezier(.23,1,.32,1) forwards; }
  @keyframes brandIn {
    0% { opacity:0; transform:translateY(20px) scaleX(1.5); letter-spacing:40px; filter:blur(8px); }
    60% { opacity:1; filter:blur(0); }
    100% { opacity:1; transform:translateY(0) scaleX(1); letter-spacing:14px; filter:blur(0); }
  }
  .tagline {
    margin-top: 14px; font-size: 13px; letter-spacing: 8px;
    color: rgba(0,255,200,0); text-transform: uppercase;
  }
  .tagline.show { animation: tagIn 1s ease-out forwards; }
  @keyframes tagIn { to { color: rgba(0,255,200,0.7); } }
  .version {
    margin-top: 8px; font-size: 11px; letter-spacing: 3px;
    color: rgba(0,255,255,0); opacity: 0;
  }
  .version.show { animation: verIn 1.2s ease-out forwards; }
  @keyframes verIn { to { opacity: 1; color: rgba(0,255,255,0.35); } }
  .cursor {
    display: inline-block; width: 2px; height: 1em;
    background: #0ff; margin-left: 4px;
    animation: blink .6s step-end infinite;
    vertical-align: text-bottom;
  }
  @keyframes blink { 0%,100%{opacity:1} 50%{opacity:0} }
</style>
</head>
<body>
<canvas id="rain"></canvas>
<canvas id="main"></canvas>
<canvas id="fx"></canvas>
<div class="scanlines"></div>
<div class="glitch-overlay" id="glitch"></div>
<div class="pixel-grid" id="pixelGrid"></div>
<div class="vignette"></div>
<div class="text-layer">
  <div class="brand" id="brand"></div>
  <div class="tagline" id="tagline">AI CODING CLI</div>
  <div class="version" id="version">v1.0.0</div>
</div>

<script>
const W = window.innerWidth, H = window.innerHeight;
const PIXEL = 3;
let globalTime = 0;

// ====== 代码雨 ======
const rainCvs = document.getElementById('rain');
const rCtx = rainCvs.getContext('2d');
rainCvs.width = W; rainCvs.height = H;
const cols = Math.floor(W / 14);
const drops = Array(cols).fill(0).map(() => Math.random() * -100);
const glyphs = 'kkcode01{}[]<>/;:=+-._#$@!アイウエオカキクケコ'.split('');

function drawRain() {
  rCtx.fillStyle = 'rgba(10,10,15,0.06)';
  rCtx.fillRect(0, 0, W, H);
  rCtx.font = '13px Courier New';
  for (let i = 0; i < cols; i++) {
    const ch = glyphs[Math.floor(Math.random() * glyphs.length)];
    const x = i * 14, y = drops[i] * 14;
    rCtx.fillStyle = Math.random() > 0.98 ? '#fff'
      : `rgba(0,${150 + Math.random()*105},${100 + Math.random()*80},${0.4 + Math.random()*0.4})`;
    rCtx.fillText(ch, x, y);
    if (y > H && Math.random() > 0.975) drops[i] = 0;
    drops[i] += 1.2;
  }
}

// ====== Canvas ======
const mainCvs = document.getElementById('main');
const ctx = mainCvs.getContext('2d');
mainCvs.width = W; mainCvs.height = H;
const fxCvs = document.getElementById('fx');
const fxCtx = fxCvs.getContext('2d');
fxCvs.width = W; fxCvs.height = H;

// ====== 粒子系统 ======
let particles = [];
let sparks = [];
let logoBounds = { x: 0, y: 0, w: 0, h: 0 };

class Particle {
  constructor(tx, ty, color, r, g, b) {
    this.tx = tx; this.ty = ty; this.color = color;
    this.or = r; this.og = g; this.ob = b;
    const a = Math.random() * Math.PI * 2;
    const d = 400 + Math.random() * 600;
    this.x = tx + Math.cos(a) * d;
    this.y = ty + Math.sin(a) * d;
    this.vx = 0; this.vy = 0;
    this.delay = Math.random() * 900;
    this.elapsed = 0;
    this.arrived = false;
    this.glowI = 0;
    this.phase = Math.random() * Math.PI * 2;
    this.waveAmp = 0.3 + Math.random() * 0.7;
    this.floatSpeed = 0.8 + Math.random() * 1.2;
    this.glitchTimer = 0;
    this.isGlitching = false;
  }
  update(dt) {
    this.elapsed += dt;
    if (this.elapsed < this.delay) return;
    if (!this.arrived) {
      const dx = this.tx - this.x, dy = this.ty - this.y;
      if (Math.abs(dx) < 0.5 && Math.abs(dy) < 0.5) {
        this.x = this.tx; this.y = this.ty;
        this.arrived = true;
        return;
      }
      this.vx = (this.vx + dx * 0.06) * 0.82;
      this.vy = (this.vy + dy * 0.06) * 0.82;
      this.x += this.vx; this.y += this.vy;
    } else {
      this.glowI = Math.min(1, this.glowI + dt * 0.002);
      // 呼吸浮动
      const t = globalTime * 0.001;
      const wave = Math.sin(t * this.floatSpeed + this.phase) * this.waveAmp;
      this.x = this.tx + Math.sin(t * 0.5 + this.phase) * wave;
      this.y = this.ty + Math.cos(t * 0.7 + this.phase) * wave;
      // 随机故障
      this.glitchTimer -= dt;
      if (this.glitchTimer <= 0) {
        this.isGlitching = Math.random() > 0.997;
        this.glitchTimer = this.isGlitching ? 50 + Math.random() * 100 : 200 + Math.random() * 2000;
      }
    }
  }
  draw(c, scanY) {
    if (this.elapsed < this.delay) {
      if (Math.random() > 0.93) {
        c.fillStyle = `rgba(0,255,255,${Math.random()*0.4})`;
        c.fillRect(this.x, this.y, PIXEL, PIXEL);
      }
      return;
    }
    let px = this.x | 0, py = this.y | 0;
    let r = this.or, g = this.og, b = this.ob;
    // 扫描线高亮
    const scanDist = Math.abs(py - scanY);
    const scanBoost = scanDist < 30 ? (1 - scanDist / 30) * 0.6 : 0;
    // 全息色偏
    if (this.arrived) {
      const hShift = Math.sin(globalTime * 0.002 + this.phase) * 15;
      r = Math.min(255, r + hShift + scanBoost * 100);
      g = Math.min(255, g + scanBoost * 200);
      b = Math.min(255, b + hShift * 0.5 + scanBoost * 255);
    }
    // 故障偏移
    if (this.isGlitching) {
      px += (Math.random() - 0.5) * 8 | 0;
      r = 255; g = Math.random() * 100 | 0; b = Math.random() * 255 | 0;
    }
    c.fillStyle = `rgb(${r|0},${g|0},${b|0})`;
    c.fillRect(px, py, PIXEL, PIXEL);
    // 到达后发光
    if (this.arrived && this.glowI > 0.3) {
      c.globalAlpha = this.glowI * 0.15 + scanBoost * 0.3;
      c.fillRect(px - 1, py - 1, PIXEL + 2, PIXEL + 2);
      c.globalAlpha = 1;
    }
  }
}

// ====== 火花粒子 ======
class Spark {
  constructor(x, y) {
    this.x = x; this.y = y;
    const a = Math.random() * Math.PI * 2;
    const spd = 1 + Math.random() * 3;
    this.vx = Math.cos(a) * spd; this.vy = Math.sin(a) * spd;
    this.life = 1; this.decay = 0.01 + Math.random() * 0.03;
  }
  update() {
    this.x += this.vx; this.y += this.vy;
    this.vx *= 0.98; this.vy *= 0.98;
    this.life -= this.decay;
  }
  draw(c) {
    if (this.life <= 0) return;
    c.fillStyle = `rgba(0,255,255,${this.life * 0.8})`;
    c.fillRect(this.x | 0, this.y | 0, 2, 2);
  }
}

// ====== Logo像素提取 ======
const LOGO_BASE64 = '';

function loadLogo() {
  return new Promise((resolve, reject) => {
    const img = new Image();
    img.onload = () => {
      const scale = Math.min(280 / img.width, 280 / img.height);
      const w = Math.floor(img.width * scale);
      const h = Math.floor(img.height * scale);
      const off = document.createElement('canvas');
      off.width = w; off.height = h;
      const oc = off.getContext('2d');
      oc.imageSmoothingEnabled = false;
      oc.drawImage(img, 0, 0, w, h);
      const d = oc.getImageData(0, 0, w, h).data;
      const ox = Math.floor((W - w * PIXEL) / 2);
      const oy = Math.floor((H - h * PIXEL) / 2) - 60;
      logoBounds = { x: ox, y: oy, w: w * PIXEL, h: h * PIXEL };
      for (let y = 0; y < h; y++) {
        for (let x = 0; x < w; x++) {
          const i = (y * w + x) * 4;
          const r = d[i], g = d[i+1], b = d[i+2], a = d[i+3];
          if (a < 30 || (r < 25 && g < 25 && b < 25)) continue;
          const px = ox + x * PIXEL, py = oy + y * PIXEL;
          const bst = 1.3;
          const cr = Math.min(255, r * bst | 0);
          const cg = Math.min(255, g * bst | 0);
          const cb = Math.min(255, b * bst | 0);
          const color = `rgb(${cr},${cg},${cb})`;
          particles.push(new Particle(px, py, color, cr, cg, cb));
        }
      }
      resolve();
    };
    img.onerror = reject;
    img.src = 'data:image/png;base64,' + LOGO_BASE64;
  });
}

// ====== 故障效果 ======
function triggerGlitch() {
  const gl = document.getElementById('glitch');
  let c = 0;
  const iv = setInterval(() => {
    const r = Math.random();
    gl.style.background = r < 0.3 ? `rgba(255,0,100,${0.05+Math.random()*0.12})`
      : r < 0.6 ? `rgba(0,255,255,${0.05+Math.random()*0.12})` : 'transparent';
    gl.style.opacity = 1;
    mainCvs.style.transform = `translate(${(Math.random()-.5)*10}px,${(Math.random()-.5)*5}px)`;
    if (++c > 18) { clearInterval(iv); gl.style.opacity = 0; mainCvs.style.transform = 'none'; }
  }, 45);
}

// ====== 霓虹脉冲环 ======
let rings = [];
function addRing(x, y, color) {
  rings.push({ x, y, r: 10, a: 0.8, color: color || '0,255,255' });
}
function drawRings() {
  rings = rings.filter(ring => {
    ring.r += 3.5; ring.a *= 0.965;
    if (ring.a < 0.01) return false;
    ctx.beginPath();
    ctx.arc(ring.x, ring.y, ring.r, 0, Math.PI * 2);
    ctx.strokeStyle = `rgba(${ring.color},${ring.a})`;
    ctx.lineWidth = 2; ctx.stroke();
    return true;
  });
}

// ====== 扫描线 ======
let scanY = -100;
function updateScan() {
  if (phase === 'alive') {
    scanY += 1.5;
    if (scanY > logoBounds.y + logoBounds.h + 50) scanY = logoBounds.y - 50;
  }
}
function drawScanLine() {
  if (phase !== 'alive' && phase !== 'text') return;
  fxCtx.clearRect(0, 0, W, H);
  // 扫描线光束
  const grad = fxCtx.createLinearGradient(0, scanY - 20, 0, scanY + 20);
  grad.addColorStop(0, 'rgba(0,255,255,0)');
  grad.addColorStop(0.5, 'rgba(0,255,255,0.08)');
  grad.addColorStop(1, 'rgba(0,255,255,0)');
  fxCtx.fillStyle = grad;
  fxCtx.fillRect(logoBounds.x - 20, scanY - 20, logoBounds.w + 40, 40);
}

// ====== 边缘火花 ======
function emitSparks() {
  if (phase !== 'alive' && phase !== 'text') return;
  if (Math.random() > 0.15) return;
  const edge = particles[Math.random() * particles.length | 0];
  if (edge && edge.arrived) {
    sparks.push(new Spark(edge.x, edge.y));
  }
}
function updateSparks() {
  sparks = sparks.filter(s => { s.update(); return s.life > 0; });
}

// ====== 打字机 ======
function typeWriter(el, text, spd = 100) {
  return new Promise(resolve => {
    let i = 0;
    const cur = document.createElement('span');
    cur.className = 'cursor'; el.appendChild(cur);
    const iv = setInterval(() => {
      if (i < text.length) {
        const s = document.createElement('span');
        s.textContent = text[i];
        el.insertBefore(s, cur);
        if (Math.random() > 0.6) {
          const g = glyphs[Math.floor(Math.random()*glyphs.length)];
          s.textContent = g;
          const ch = text[i];
          setTimeout(() => { s.textContent = ch; }, 80);
        }
        i++;
      } else { clearInterval(iv); setTimeout(() => { cur.remove(); resolve(); }, 500); }
    }, spd);
  });
}

// ====== 主循环 ======
let phase = 'rain', lastT = 0, phaseT = 0;

function animate(t) {
  const dt = lastT ? t - lastT : 16;
  lastT = t; phaseT += dt; globalTime = t;
  ctx.clearRect(0, 0, W, H);
  drawRain();

  if (phase !== 'rain') {
    updateScan();
    emitSparks();
    updateSparks();
    for (const p of particles) { p.update(dt); p.draw(ctx, scanY); }
    for (const s of sparks) { s.draw(ctx); }
    drawScanLine();
  }
  drawRings();

  // 周期性脉冲和故障（alive阶段）
  if (phase === 'alive' || phase === 'text') {
    if (Math.random() > 0.998) {
      const cx = logoBounds.x + logoBounds.w / 2;
      const cy = logoBounds.y + logoBounds.h / 2;
      addRing(cx, cy);
    }
    if (Math.random() > 0.9985) triggerGlitch();
  }

  // 代码雨淡出
  if (phase === 'assemble' && phaseT > 1500) {
    const op = Math.max(0.08, parseFloat(rainCvs.style.opacity || 1) - 0.008);
    rainCvs.style.opacity = op;
  }
  requestAnimationFrame(animate);
}

const sleep = ms => new Promise(r => setTimeout(r, ms));

// ====== 启动序列 ======
async function start() {
  requestAnimationFrame(animate);
  await sleep(1500);

  await loadLogo();

  phase = 'assemble'; phaseT = 0;
  document.getElementById('pixelGrid').classList.add('show');

  await sleep(800);
  triggerGlitch();
  await sleep(600);
  triggerGlitch();
  await sleep(1800);

  // 发光脉冲 - logo组装完成
  const cx = logoBounds.x + logoBounds.w / 2;
  const cy = logoBounds.y + logoBounds.h / 2;
  addRing(cx, cy);
  await sleep(200);
  addRing(cx, cy, '0,255,200');
  triggerGlitch();
  await sleep(400);
  addRing(cx, cy, '100,200,255');

  // 进入alive阶段 - logo持续动画
  phase = 'alive';
  scanY = logoBounds.y - 50;
  await sleep(600);

  // 文字
  phase = 'text';
  const brand = document.getElementById('brand');
  brand.classList.add('show');
  brand.style.opacity = 1;
  await typeWriter(brand, 'KKCODE', 120);
  await sleep(300);
  document.getElementById('tagline').classList.add('show');
  await sleep(500);
  document.getElementById('version').classList.add('show');
  await sleep(300);
  addRing(cx, cy);
  triggerGlitch();

  // 保持alive状态，logo持续动画
  phase = 'alive';
}

start();
</script>
</body>
</html>
